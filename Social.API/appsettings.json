{
  "ConnectionStrings": {
    "UserManagementDbConnectionString": ""
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Jwt": {
    "Secret": "",
    "Issuer": "",
    "Audience": "",
    "TokenLifeTimeInMinutes": 40
  },
  "Bucket": {
    "Host": "",
    "ProfilePictureBucket": "",
    "User": "",
    "Password": ""
  },
  "RabbitMq": {
    "Host": "",
    "Username": "",
    "Password": ""
  },
  "ApiBaseUrls": {
    "IdentityServiceUrl": "https://localhost:44306/"
  },
  "AllowedHosts": "*",
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Sinks.Grafana.Loki"
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "GrafanaLoki",
        "Args": {
          "uri": "",
          "labels": [
            {
              "key": "app",
              "value": "cypherly.usermanagement.svc"
            }
          ]
        }
      }
    ],
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithThreadId",
      "WithProperty(\"app\", \"cypherly.usermanagement.svc\")",
      "WithProperty(\"env\", \"prod\")"
    ],
    "Properties": {
      "Application": "cypherly.usermanagement.svc"
    }
  },
  "AllowedOrigins": [
    "http://localhost:8080",
    "http://localhost:8081",
    "http://localhost:8082",
    "http://localhost:8083",
    "http://localhost:8084"
  ]
}
